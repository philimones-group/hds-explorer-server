          <item>
            <label ref="jr:itext('/pregnancy_outcome_ext/childs/child_outcome_type/MIS:label')"/>
            <value>MIS</value>
          </item>
          <item>
            <label ref="jr:itext('/pregnancy_outcome_ext/childs/child_outcome_type/ABT:label')"/>
            <value>ABT</value>
          </item>
        </select1>
        <input ref="/pregnancy_outcome_ext/childs/child_code">
          <label ref="jr:itext('/pregnancy_outcome_ext/childs/child_code:label')"/>
