          </item>
          <item>
            <label ref="jr:itext('/pregnancy_outcome_ext/childs/child_gender/F:label')"/>
            <value>F</value>
          </item>
        </select1>
      </repeat>
    </group>
    <input ref="/pregnancy_outcome_ext/custom_quest1">
      <label ref="jr:itext('/pregnancy_outcome_ext/custom_quest1:label')"/>
    </input>
    <input ref="/pregnancy_outcome_ext/custom_quest2">
      <label ref="jr:itext('/pregnancy_outcome_ext/custom_quest2:label')"/>
    </input>
