          <text id="/pregnancy_registration_ext/start:label">
            <value>-</value>
          </text>
          <text id="/pregnancy_registration_ext/pregnancy_status:label">
            <value>Pregnancy Status</value>
          </text>
          <text id="/pregnancy_registration_ext/visit_code:label">
