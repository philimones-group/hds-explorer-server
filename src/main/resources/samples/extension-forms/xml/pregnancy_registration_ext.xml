      </itext>
      <instance>
        <pregnancy_registration_ext id="pregnancy_registration_ext" version="1">
          <start/>
          <end/>
          <deviceid/>
          <collected_id/>
          <visit_code/>
          <pregnancy_code/>
          <mother_code/>
          <mother_name/>
          <pregnancy_status/>
          <custom_quest1/>
          <custom_quest2/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </pregnancy_registration_ext>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/pregnancy_registration_ext/start" required="true()" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/pregnancy_registration_ext/end" required="true()" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/pregnancy_registration_ext/deviceid" required="true()" type="string"/>
      <bind nodeset="/pregnancy_registration_ext/collected_id" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_registration_ext/visit_code" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_registration_ext/pregnancy_code" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_registration_ext/mother_code" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_registration_ext/mother_name" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_registration_ext/pregnancy_status" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/pregnancy_registration_ext/custom_quest1" type="string"/>
      <bind nodeset="/pregnancy_registration_ext/custom_quest2" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/pregnancy_registration_ext/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;HDS_&quot;, /pregnancy_registration_ext/mother_code ,&quot;_&quot;,&quot;PregRegistration_Extension&quot;)" nodeset="/pregnancy_registration_ext/meta/instanceName" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/pregnancy_registration_ext/collected_id">
      <label ref="jr:itext('/pregnancy_registration_ext/collected_id:label')"/>
    </input>
    <input ref="/pregnancy_registration_ext/visit_code">
      <label ref="jr:itext('/pregnancy_registration_ext/visit_code:label')"/>
    </input>
    <input ref="/pregnancy_registration_ext/pregnancy_code">
      <label ref="jr:itext('/pregnancy_registration_ext/pregnancy_code:label')"/>
    </input>
    <input ref="/pregnancy_registration_ext/mother_code">
      <label ref="jr:itext('/pregnancy_registration_ext/mother_code:label')"/>
    </input>
    <input ref="/pregnancy_registration_ext/mother_name">
      <label ref="jr:itext('/pregnancy_registration_ext/mother_name:label')"/>
    </input>
    <select1 ref="/pregnancy_registration_ext/pregnancy_status">
      <label ref="jr:itext('/pregnancy_registration_ext/pregnancy_status:label')"/>
      <item>
        <label ref="jr:itext('/pregnancy_registration_ext/pregnancy_status/PREGNANT:label')"/>
        <value>PREGNANT</value>
      </item>
      <item>
        <label ref="jr:itext('/pregnancy_registration_ext/pregnancy_status/DELIVERED:label')"/>
        <value>DELIVERED</value>
      </item>
      <item>
        <label ref="jr:itext('/pregnancy_registration_ext/pregnancy_status/LOST_TRACK:label')"/>
        <value>LOST_TRACK</value>
      </item>
    </select1>
    <input ref="/pregnancy_registration_ext/custom_quest1">
      <label ref="jr:itext('/pregnancy_registration_ext/custom_quest1:label')"/>
    </input>
    <input ref="/pregnancy_registration_ext/custom_quest2">
      <label ref="jr:itext('/pregnancy_registration_ext/custom_quest2:label')"/>
    </input>
  </h:body>
</h:html>
