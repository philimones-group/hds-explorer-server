<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>HDS Inmigration Registration Extension</h:title>
    <model>
      <itext>
        <translation lang="Portuguese">
          <text id="/inmigration_ext/migration_type:label">
            <value>Tipo de Imigração</value>
          </text>
          <text id="/inmigration_ext/collected_id:label">
            <value>Id do Formulário Mãe</value>
          </text>
          <text id="/inmigration_ext/member_name:label">
            <value>Nome do Membro</value>
          </text>
          <text id="/inmigration_ext/migration_type/XEN:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/ext_migration_type/REENTRY:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/end:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/deviceid:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/visit_code:label">
            <value>Código da Visita</value>
          </text>
          <text id="/inmigration_ext/custom_quest1:label">
            <value>Custom Question 1</value>
          </text>
          <text id="/inmigration_ext/start:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/ext_migration_type/ENTRY:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/migration_type/ENT:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/custom_quest2:label">
            <value>Custom Question 2</value>
          </text>
          <text id="/inmigration_ext/ext_migration_type:label">
            <value>Tipo de Imigração Externa</value>
          </text>
          <text id="/inmigration_ext/member_code:label">
            <value>Código do Membro</value>
          </text>
        </translation>
        <translation lang="pt">
          <text id="/inmigration_ext/visit_code:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/migration_type:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/collected_id:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/member_name:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/start:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/migration_type/ENT:label">
            <value>Imigração Interna</value>
          </text>
          <text id="/inmigration_ext/end:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/deviceid:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/ext_migration_type/REENTRY:label">
            <value>Retorno ao DSS</value>
          </text>
          <text id="/inmigration_ext/custom_quest1:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/migration_type/XEN:label">
            <value>Imigração Externa</value>
          </text>
          <text id="/inmigration_ext/ext_migration_type/ENTRY:label">
            <value>Primeira Entrada no DSS</value>
          </text>
          <text id="/inmigration_ext/custom_quest2:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/ext_migration_type:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/member_code:label">
            <value>-</value>
          </text>
        </translation>
        <translation default="true()" lang="English">
          <text id="/inmigration_ext/migration_type:label">
            <value>Migration Type</value>
          </text>
          <text id="/inmigration_ext/collected_id:label">
            <value>Core Form Id</value>
          </text>
          <text id="/inmigration_ext/member_name:label">
            <value>Member Name</value>
          </text>
          <text id="/inmigration_ext/migration_type/XEN:label">
            <value>External InMigration</value>
          </text>
          <text id="/inmigration_ext/ext_migration_type/REENTRY:label">
            <value>Return to DSS</value>
          </text>
          <text id="/inmigration_ext/migration_type/ENT:label">
            <value>Internal InMigration</value>
          </text>
          <text id="/inmigration_ext/end:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/deviceid:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/visit_code:label">
            <value>Visit Code</value>
          </text>
          <text id="/inmigration_ext/custom_quest1:label">
            <value>Custom Question 1</value>
          </text>
          <text id="/inmigration_ext/start:label">
            <value>-</value>
          </text>
          <text id="/inmigration_ext/ext_migration_type/ENTRY:label">
            <value>First Entry into the DSS</value>
          </text>
          <text id="/inmigration_ext/custom_quest2:label">
            <value>Custom Question 2</value>
          </text>
          <text id="/inmigration_ext/ext_migration_type:label">
            <value>External InMigration Type</value>
          </text>
          <text id="/inmigration_ext/member_code:label">
            <value>Member Code</value>
          </text>
        </translation>
      </itext>
      <instance>
        <inmigration_ext id="inmigration_ext" version="1">
          <start/>
          <end/>
          <deviceid/>
          <collected_id/>
          <visit_code/>
          <member_code/>
          <member_name/>
          <migration_type/>
          <ext_migration_type/>
          <custom_quest1/>
          <custom_quest2/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </inmigration_ext>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/inmigration_ext/start" required="true()" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/inmigration_ext/end" required="true()" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/inmigration_ext/deviceid" required="true()" type="string"/>
      <bind nodeset="/inmigration_ext/collected_id" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/inmigration_ext/visit_code" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/inmigration_ext/member_code" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/inmigration_ext/member_name" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/inmigration_ext/migration_type" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/inmigration_ext/ext_migration_type" readonly="true()" relevant=" /inmigration_ext/migration_type ='XEN'" required="true()" type="select1"/>
      <bind nodeset="/inmigration_ext/custom_quest1" type="string"/>
      <bind nodeset="/inmigration_ext/custom_quest2" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/inmigration_ext/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;HDS_&quot;, /inmigration_ext/member_code ,&quot;_&quot;,&quot;Inmigration_Extension&quot;)" nodeset="/inmigration_ext/meta/instanceName" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/inmigration_ext/collected_id">
      <label ref="jr:itext('/inmigration_ext/collected_id:label')"/>
    </input>
    <input ref="/inmigration_ext/visit_code">
      <label ref="jr:itext('/inmigration_ext/visit_code:label')"/>
    </input>
    <input ref="/inmigration_ext/member_code">
      <label ref="jr:itext('/inmigration_ext/member_code:label')"/>
    </input>
    <input ref="/inmigration_ext/member_name">
      <label ref="jr:itext('/inmigration_ext/member_name:label')"/>
    </input>
    <select1 ref="/inmigration_ext/migration_type">
      <label ref="jr:itext('/inmigration_ext/migration_type:label')"/>
      <item>
        <label ref="jr:itext('/inmigration_ext/migration_type/ENT:label')"/>
        <value>ENT</value>
      </item>
      <item>
        <label ref="jr:itext('/inmigration_ext/migration_type/XEN:label')"/>
        <value>XEN</value>
      </item>
    </select1>
    <select1 ref="/inmigration_ext/ext_migration_type">
      <label ref="jr:itext('/inmigration_ext/ext_migration_type:label')"/>
      <item>
        <label ref="jr:itext('/inmigration_ext/ext_migration_type/ENTRY:label')"/>
        <value>ENTRY</value>
      </item>
      <item>
        <label ref="jr:itext('/inmigration_ext/ext_migration_type/REENTRY:label')"/>
        <value>REENTRY</value>
      </item>
    </select1>
    <input ref="/inmigration_ext/custom_quest1">
      <label ref="jr:itext('/inmigration_ext/custom_quest1:label')"/>
    </input>
    <input ref="/inmigration_ext/custom_quest2">
      <label ref="jr:itext('/inmigration_ext/custom_quest2:label')"/>
    </input>
  </h:body>
</h:html>
