        <translation default="true()" lang="English">
          <text id="/household_proxy_head_ext/collected_id:label">
            <value>Core Form Id</value>
          </text>
          <text id="/household_proxy_head_ext/visit_code:label">
            <value>Visit Code</value>
          </text>
          <text id="/household_proxy_head_ext/deviceid:label">
            <value>-</value>
          </text>
          <text id="/household_proxy_head_ext/proxy_head_role:label">
            <value>Proxy Head Role</value>
          </text>
          <text id="/household_proxy_head_ext/proxy_head_name:label">
            <value>Proxy Head Name</value>
          </text>
          <text id="/household_proxy_head_ext/custom_quest2:label">
            <value>Custom Question 2</value>
          </text>
          <text id="/household_proxy_head_ext/household_code:label">
            <value>Household Code</value>
          </text>
          <text id="/household_proxy_head_ext/proxy_head_code:label">
            <value>Proxy Head Code</value>
          </text>
          <text id="/household_proxy_head_ext/proxy_head_type:label">
            <value>Proxy Head Type</value>
          </text>
          <text id="/household_proxy_head_ext/start:label">
            <value>-</value>
          </text>
          <text id="/household_proxy_head_ext/end:label">
            <value>-</value>
          </text>
          <text id="/household_proxy_head_ext/custom_quest1:label">
            <value>Custom Question 1</value>
          </text>
          <text id="/household_proxy_head_ext/household_name:label">
            <value>Household Name</value>
          </text>
        </translation>
      </itext>
      <instance>
        <household_proxy_head_ext id="household_proxy_head_ext" version="1">
          <start/>
          <end/>
          <deviceid/>
          <collected_id/>
          <visit_code/>
          <household_code/>
          <household_name/>
          <proxy_head_type/>
          <proxy_head_code/>
          <proxy_head_name/>
          <proxy_head_role/>
          <custom_quest1/>
          <custom_quest2/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </household_proxy_head_ext>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/household_proxy_head_ext/start" required="true()" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/household_proxy_head_ext/end" required="true()" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/household_proxy_head_ext/deviceid" required="true()" type="string"/>
      <bind nodeset="/household_proxy_head_ext/collected_id" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_proxy_head_ext/visit_code" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_proxy_head_ext/household_code" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_proxy_head_ext/household_name" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_proxy_head_ext/proxy_head_type" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_proxy_head_ext/proxy_head_code" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_proxy_head_ext/proxy_head_name" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_proxy_head_ext/proxy_head_role" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_proxy_head_ext/custom_quest1" type="string"/>
      <bind nodeset="/household_proxy_head_ext/custom_quest2" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/household_proxy_head_ext/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;HDS_&quot;, /household_proxy_head_ext/household_code ,&quot;_&quot;,&quot;ChangeProxy_Extension&quot;)" nodeset="/household_proxy_head_ext/meta/instanceName" type="string"/>
    </model>
  </h:head>
