<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>HDS Change Head of Household Extension</h:title>
    <model>
      <itext>
        <translation lang="Portuguese">
          <text id="/household_relocation_ext/head_code:label">
            <value>Códido do Chefe do agregado</value>
          </text>
          <text id="/household_relocation_ext/origin_code:label">
            <value>Código do Agregado de Origem</value>
          </text>
          <text id="/household_relocation_ext/head_name:label">
            <value>Nome do Chefe do agregado</value>
          </text>
          <text id="/household_relocation_ext/custom_quest1:label">
            <value>Custom Question 1</value>
          </text>
          <text id="/household_relocation_ext/destination_code:label">
            <value>Código do Agregado de Destino</value>
          </text>
          <text id="/household_relocation_ext/deviceid:label">
            <value>-</value>
          </text>
          <text id="/household_relocation_ext/custom_quest2:label">
            <value>Custom Question 2</value>
          </text>
          <text id="/household_relocation_ext/collected_id:label">
            <value>Id do Formulário Mãe</value>
          </text>
          <text id="/household_relocation_ext/end:label">
            <value>-</value>
          </text>
          <text id="/household_relocation_ext/start:label">
            <value>-</value>
          </text>
          <text id="/household_relocation_ext/visit_code:label">
            <value>Código da Visita</value>
          </text>
        </translation>
        <translation default="true()" lang="English">
          <text id="/household_relocation_ext/head_code:label">
            <value>Head of Household Code</value>
          </text>
          <text id="/household_relocation_ext/origin_code:label">
            <value>Origin Household Code</value>
          </text>
          <text id="/household_relocation_ext/head_name:label">
            <value>Head of Household Name</value>
          </text>
          <text id="/household_relocation_ext/custom_quest1:label">
            <value>Custom Question 1</value>
          </text>
          <text id="/household_relocation_ext/destination_code:label">
            <value>Destination Household Code</value>
          </text>
          <text id="/household_relocation_ext/deviceid:label">
            <value>-</value>
          </text>
          <text id="/household_relocation_ext/custom_quest2:label">
            <value>Custom Question 2</value>
          </text>
          <text id="/household_relocation_ext/collected_id:label">
            <value>Core Form Id</value>
          </text>
          <text id="/household_relocation_ext/end:label">
            <value>-</value>
          </text>
          <text id="/household_relocation_ext/start:label">
            <value>-</value>
          </text>
          <text id="/household_relocation_ext/visit_code:label">
            <value>Visit Code</value>
          </text>
        </translation>
      </itext>
      <instance>
        <household_relocation_ext id="household_relocation_ext" version="1">
          <start/>
          <end/>
          <deviceid/>
          <collected_id/>
          <visit_code/>
          <origin_code/>
          <destination_code/>
          <head_code/>
          <head_name/>
          <custom_quest1/>
          <custom_quest2/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </household_relocation_ext>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/household_relocation_ext/start" required="true()" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/household_relocation_ext/end" required="true()" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/household_relocation_ext/deviceid" required="true()" type="string"/>
      <bind nodeset="/household_relocation_ext/collected_id" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_relocation_ext/visit_code" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_relocation_ext/origin_code" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_relocation_ext/destination_code" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_relocation_ext/head_code" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_relocation_ext/head_name" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/household_relocation_ext/custom_quest1" type="string"/>
      <bind nodeset="/household_relocation_ext/custom_quest2" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/household_relocation_ext/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;HDS_&quot;, /household_relocation_ext/origin_code ,&quot;_&quot;,&quot;HouseRelocation_Extension&quot;)" nodeset="/household_relocation_ext/meta/instanceName" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/household_relocation_ext/collected_id">
      <label ref="jr:itext('/household_relocation_ext/collected_id:label')"/>
    </input>
    <input ref="/household_relocation_ext/visit_code">
      <label ref="jr:itext('/household_relocation_ext/visit_code:label')"/>
    </input>
    <input ref="/household_relocation_ext/origin_code">
      <label ref="jr:itext('/household_relocation_ext/origin_code:label')"/>
    </input>
    <input ref="/household_relocation_ext/destination_code">
      <label ref="jr:itext('/household_relocation_ext/destination_code:label')"/>
    </input>
    <input ref="/household_relocation_ext/head_code">
      <label ref="jr:itext('/household_relocation_ext/head_code:label')"/>
    </input>
    <input ref="/household_relocation_ext/head_name">
      <label ref="jr:itext('/household_relocation_ext/head_name:label')"/>
    </input>
    <input ref="/household_relocation_ext/custom_quest1">
      <label ref="jr:itext('/household_relocation_ext/custom_quest1:label')"/>
    </input>
    <input ref="/household_relocation_ext/custom_quest2">
      <label ref="jr:itext('/household_relocation_ext/custom_quest2:label')"/>
    </input>
  </h:body>
</h:html>
